{
  "manifest_version": 3,
  "name": "OverReleaf Extension",
  "version": "1.0",
  "description": "A Chrome extension for OverReleaf.",
  "permissions": ["storage", "activeTab", "scripting"],
  "web_accessible_resources": [
    {
      "resources": ["injectDiv.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
